<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Ja3Bu9 Quotes</title>
    <link rel="stylesheet" href="/style.css">
    <link href="https://fonts.googleapis.com/css?family=Shadows+Into+Light&display=swap" rel="stylesheet">
</head>

<body>

    <h1>Quotes :</h1>
   


        <table id="myTable" >  
            <thead>  
                <tr class="table100-head">  
                    <th class="column">ID</th>  
                    <th class="column">text</th>  
                    <th class="column">source</th>  
                    <th class="column">Auteur ID</th>  

                </tr>  
            </thead>  
             <tbody>  
             <% quotes.forEach(function (citation) { %>
                <tr>  
                    <td class="column"><%= citation.id %></td>  
                    <td class="column"><%= citation.text %></td>  
                    <td class="column"><%= citation.source %></td>
                    <td class="column"><%= citation.nom %></td>

                </tr>                                   
             <% }) %>
             </tbody>
        </table>  
       





        


</body>

</html>